{
  "development": {
    "dialect": "sqlite",
    "storage": "oidc.db"
  },
  "test": {
    "dialect": "sqlite",
    "storage": "oidc-test.db"
  },
  "production": {
    "username": "${process.env.OIDC_DB_USER}",
    "password": "${process.env.OIDC_DB_PASS}",
    "database": "${process.env.OIDC_DB_NAME}",
    "host": "${process.env.DB_HOST}",
    "port": "${process.env.DB_PORT}",
    "dialect": "postgres"
  }
}
